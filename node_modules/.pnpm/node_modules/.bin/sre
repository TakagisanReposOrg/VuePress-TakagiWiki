#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/bin/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules/speech-rule-engine/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/speech-rule-engine@4.0.7/node_modules:/workspaces/VuePress-TakagiWiki/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../speech-rule-engine/bin/sre" "$@"
else
  exec node  "$basedir/../speech-rule-engine/bin/sre" "$@"
fi
